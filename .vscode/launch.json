{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/test.exe",
          "args": [],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "externalConsole": false,
          "MIMode": "gdb",
          "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
